

# Required for Telegram session setup
API_ID=
API_HASH=

# Tapping and Turbo Settings
RANDOM_TURBO_TAPS=[5000,200000]          # Example range for turbo taps
RANDOM_TAPS=[5000,200000]                # Example range for taps

# Auto-apply settings for Turbo and Energy
AUTO_APPLY_TURBO=true               # Automatically apply turbo if true
AUTO_APPLY_ENERGY=true              # Automatically apply energy if true

# Damage, Recharge, and Energy Upgrade Settings
AUTO_UPGRADE_DAMAGE=true            # Set to true to auto-upgrade damage
MAX_DAMAGE_LEVEL=5000000000000000000000000000000                  # Max level for damage upgrades

AUTO_UPGRADE_RECHARGE=true          # Set to true to auto-upgrade recharge
MAX_RECHARGE_LEVEL=50000000000000000                # Max level for recharge upgrades

AUTO_UPGRADE_ENERGY=true            # Set to true to auto-upgrade energy
MAX_ENERGY_LEVEL=50000000000000000                  # Max level for energy upgrades

# Automatic Actions
AUTO_BUY_TAPBOT=true                # Automatically buy TapBot if coins available
AUTO_CLAIM_AND_START_TAPBOT=true    # Automatically claim and start TapBot
AUTO_SPIN=true                      # Enable automatic spins
AUTO_COMPLETE_TASKS=true            # Enable auto task completion

# Energy and Request Timing
MIN_AVAILABLE_ENERGY=100            # Minimum energy level before delay
SLEEP_BETWEEN_REQUESTS=[10,26]      # Sleep time range between requests
DELAY_BETWEEN_TURBO=[10,26]         # Delay between turbo taps
DELAY_BETWEEN_TAPS=[10,26]          # Delay between taps
DELAY_BETWEEN_TASKS=[10,26]         # Delay between tasks
DELAY_BETWEEN_STARTING_BOT=[10,26]  # Delay between starting the bot

# Proxy Settings - disabled since you donâ€™t have a proxy
USE_REGISTRATION_PROXY=false
USE_PROXY_FROM_JS_FILE=false
USE_PROXY_FROM_TXT_FILE=false

# Miscellaneous
USE_CODES_FROM_FILE=true            # Enable using codes from a file
AUTO_TAPPING=true                   # Enable auto-tapping
MAX_CONCURRENT_ACCOUNT=1            # Max accounts to use concurrently
USE_NON_THREAD=false                # Disable non-threaded mode
SLEEP_BETWEEN_NON_THREADS=5         # Sleep duration between non-threaded actions
